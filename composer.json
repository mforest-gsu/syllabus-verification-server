{
  "name": "mforest-gsu/syllabus-verification-server",
  "description": "",
  "type": "library",
  "keywords": [],
  "homepage": "https://github.com/mforest-gsu/syllabus-verification-server",
  "license": "MIT",
  "authors": [
    {
      "name": "Melody Kimball",
      "role": "lead"
    }
  ],
  "support": {
    "issues": "https://github.com/mforest-gsu/syllabus-verification-server/issues",
    "wiki": "https://github.com/mforest-gsu/syllabus-verification-server/wiki",
    "source": "https://github.com/mforest-gsu/syllabus-verification-server"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": "^8.1",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "doctrine/dbal": "^4.2",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/doctrine-migrations-bundle": "^3.3",
    "doctrine/orm": "^3.3",
    "mjfk23/gadget-console": "^2.0",
    "paragonie/sodium_compat": "^2.1",
    "symfony/console": "6.4.*",
    "symfony/dotenv": "6.4.*",
    "symfony/flex": "^2",
    "symfony/framework-bundle": "6.4.*",
    "symfony/monolog-bundle": "*",
    "symfony/requirements-checker": "2.0.1",
    "symfony/runtime": "6.4.*",
    "symfony/yaml": "6.4.*"
  },
  "require-dev": {
    "mjfk23/gadget-code-style": "^1.0",
    "symfony/maker-bundle": "^1.61"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "symfony/flex": true,
      "symfony/runtime": true,
      "phpstan/extension-installer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "apcu-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "Gsu\\SyllabusVerification\\": "src/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd",
      "requirements-checker": "script"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "phpcs": "vendor/bin/phpcs",
    "phpstan": "vendor/bin/phpstan analyse -c phpstan.neon",
    "start": [
      "Composer\\Config::disableProcessTimeout",
      "@php -S localhost:8080 -t public"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "6.4.*",
      "endpoint": [
        "https://raw.githubusercontent.com/mjfk23/symfony-recipes/main/index.json",
        "flex://defaults"
      ]
    }
  }
}
